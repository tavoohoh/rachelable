{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    
    "paths": {
      "@comp/*": [
        "src/app/components/*"
      ],
      "@cons/*": [
        "src/app/constants/*"
      ],
      "@enum/*": [
        "src/app/enums/*"
      ],
      "@page/*": [
        "src/app/pages/*"
      ],
      "@serv/*": [
        "src/app/services/*"
      ]
    },
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
